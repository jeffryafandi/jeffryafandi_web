.navInput:checked ~ .mainWrapper .mainMenu{width:68px}
.navInput:checked ~ .mainWrapper .htmlMenu .close,
.navInput:checked ~ .mainWrapper .mainMenu #LinkList002{width:calc(68px - var(--nav-border))}
